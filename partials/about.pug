mixin award(link, date, title, description, project)
  li.about__section__list__item
    span.about__section__list__dash(data-animation="line" data-animation-threshold="1" data-animation-target=".about__section__list__item")

    a.about__section__list__link(
      data-animation="y"
      data-animation-threshold="1"
      data-animation-target=".about__section__list__item"
      href=link target="_blank"
      rel="noreferrer noopener"
    )
      span.about__section__list__column=date
      span.about__section__list__column=title
      span.about__section__list__column=description
      span.about__section__list__column=project

mixin press(link, title, description)
  li.about__section__list__item
    span.about__section__list__dash(data-animation="line" data-animation-threshold="1" data-animation-target=".about__section__list__item")

    a.about__section__list__link.about__section__list__link--article(
      data-animation="y"
      data-animation-threshold="1"
      data-animation-target=".about__section__list__item"
      href=link target="_blank"
      rel="noreferrer noopener"
    )
      span.about__section__list__column=title
      span.about__section__list__column=description

